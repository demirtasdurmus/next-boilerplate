{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "next/core-web-vitals",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "@typescript-eslint/return-await": "off",
    "@typescript-eslint/naming-convention": "off",
    "no-underscore-dangle": "off"
  }
}
